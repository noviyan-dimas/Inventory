<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <WebView
        android:id="@+id/web_dashboard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/multiple_actions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        fab:fab_addButtonColorNormal="@color/colorPrimary"
        fab:fab_addButtonColorPressed="@color/blue_semi_transparent"
        fab:fab_addButtonPlusIconColor="@color/white"
        fab:fab_labelStyle="@style/CardView.Light"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp">

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/colorPrimary"
            fab:fab_title="Home"
            fab:fab_icon="@drawable/ic_home_white_24dp"
            fab:fab_colorPressed="@color/blue_semi_transparent"/>

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/dashboard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/colorPrimary"
            fab:fab_title="Dashboard"
            fab:fab_icon="@drawable/ic_dashboard_white_24dp"
            fab:fab_colorPressed="@color/blue_semi_transparent"/>

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/Upload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/colorPrimary"
            fab:fab_title="Upload"
            fab:fab_icon="@drawable/ic_upload_white_24dp"
            fab:fab_colorPressed="@color/blue_semi_transparent"/>

    </com.getbase.floatingactionbutton.FloatingActionsMenu>

</RelativeLayout>